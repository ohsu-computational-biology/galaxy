<dataSource xmlns="http://edu.ohsu.ccc/dataSource/xml">
    <columns>
        <column>cccDid</column>
        <column>omeroHost</column>
        <column>omeroImageId</column>
	<column>primarySite</column>
        <column>specimenType</column>
        <column>gender</column>
        <column>age</column>
        <column>tumorGrade</column>
        <column>tumorStage</column>
    </columns>
    <labkeyInstances>
        <labkeyInstance>
	<name>Austin</name>
            <baseUrl>https://austin-gateway.ccc.org:9090/labkey</baseUrl>			
            <containerPath>ccc</containerPath>
            <schemaName>lists</schemaName>
            <queryName>images</queryName>
	    <columnMappings>
	        <columnMapping columnName="cccDid" fieldKey="CCC_DID"/>
	    </columnMappings>
	</labkeyInstance>
        <labkeyInstance>
	<name>Portland</name>
            <baseUrl>https://pdx-gateway.ccc.org:9090/labkey</baseUrl>
            <containerPath>ccc</containerPath>
            <schemaName>lists</schemaName>
            <queryName>images</queryName>
            <columnMappings>
		<columnMapping columnName="cccDid" fieldKey="CCC_DID"/>
	    </columnMappings>
	</labkeyInstance>
    </labkeyInstances>
</dataSource>
