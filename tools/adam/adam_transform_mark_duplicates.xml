<tool id="adam_transform_mark_duplicates" name="Mark Duplicate Reads" version="1.0">
  <description>Mark Duplicate Reads in the adam file</description>
  <command interpreter="bash">
    adam_transform_mark_duplicates.sh  $input $output
  </command>
  <inputs>
    <param format="adam" name="input" type="data" label="HDFS_INPUT_FILE" help="--transform_mark_duplicates HDFS_INPUT_FILE help--"/>
  </inputs>
  <outputs>
    <data format="mkdup.adam" name="output" metadata_source="input"/>
  </outputs>
  <help>
  -- transform_mark_duplicates long help WIP --
  </help>
</tool>
